pipeline {
    agent any

    stages {
        stage('git checkout') {
            steps {
              git branch: 'main', url: 'https://github.com/Sushmagowdaaa/maven_Project.git' 
            }
        }
        stage('build') {
            steps {
              sh 'mvn clean install'
            }
        }
        stage('test') {
            steps {
                echo 'this is testing phase'
            }
        }
        stage('deploy') {
            steps {
                echo 'this deploying phase'
            }
        }
    }
}
